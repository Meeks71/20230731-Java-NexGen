package week1.day1;

/**
 *
 * public - Accessible anywhere (any package, class, etc.)
 * 
 * private - Only available within the class
 *
 */

public class Person {
	private String name;
}

class A {
	public static void main(String[] args) {
		Person person = new Person();
		System.out.println(person.name);
	}
}
